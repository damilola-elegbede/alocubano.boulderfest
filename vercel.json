{
  "buildCommand": "node vercel-build.cjs",
  "outputDirectory": "public",
  "framework": null,
  "devCommand": "npm run start:local",
  "crons": [
    {
      "path": "/api/sheets/scheduled-sync",
      "schedule": "*/15 * * * *"
    }
  ],
  "functions": {
    "api/gallery.js": {
      "maxDuration": 10
    },
    "api/featured-photos.js": {
      "maxDuration": 10
    },
    "api/debug.js": {
      "maxDuration": 5
    },
    "api/cache-warm.js": {
      "maxDuration": 15
    },
    "api/image-proxy/[fileId].js": {
      "maxDuration": 10
    },
    "api/payments/create-payment-intent.js": {
      "maxDuration": 10
    },
    "api/payments/stripe-webhook.js": {
      "maxDuration": 30
    },
    "api/config/stripe-public.js": {
      "maxDuration": 5
    },
    "api/payments/checkout-success.js": {
      "maxDuration": 10
    },
    "api/payments/checkout-cancel.js": {
      "maxDuration": 10
    },
    "api/payments/create-checkout-session.js": {
      "maxDuration": 15
    },
    "api/tickets/index.js": {
      "maxDuration": 10
    },
    "api/tickets/action-token.js": {
      "maxDuration": 10
    },
    "api/tickets/cancel.js": {
      "maxDuration": 10
    },
    "api/tickets/qr-code.js": {
      "maxDuration": 5
    },
    "api/tickets/transfer.js": {
      "maxDuration": 10
    },
    "api/tickets/validate.js": {
      "maxDuration": 10
    },
    "api/tickets/apple-wallet/[ticketId].js": {
      "maxDuration": 10
    },
    "api/tickets/google-wallet/[ticketId].js": {
      "maxDuration": 10
    },
    "api/wallet/[ticketId].js": {
      "maxDuration": 10
    },
    "api/wallet/apple/[ticketId].js": {
      "maxDuration": 10
    },
    "api/wallet/google/[ticketId].js": {
      "maxDuration": 10
    },
    "api/admin/**/*.js": {
      "maxDuration": 15
    },
    "api/email/**/*.js": {
      "maxDuration": 20
    },
    "api/registration/**/*.js": {
      "maxDuration": 15
    },
    "api/health/**/*.js": {
      "maxDuration": 5
    },
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "images": {
    "sizes": [16, 32, 48, 64, 96, 128, 256, 384],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "*.vercel.app"
      }
    ]
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/favicon.ico",
      "destination": "/images/favicon.ico"
    },
    {
      "source": "/gallery-data/:path*",
      "destination": "/public/gallery-data/:path*"
    },
    {
      "source": "/featured-photos.json",
      "destination": "/public/featured-photos.json"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/home",
      "destination": "/home.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/boulder-fest-2026/home",
      "destination": "/home.html"
    },
    {
      "source": "/boulder-fest-2026/about",
      "destination": "/about.html"
    },
    {
      "source": "/boulder-fest-2026/donations",
      "destination": "/donations.html"
    },
    {
      "source": "/boulder-fest-2025",
      "destination": "/boulder-fest-2025-index.html"
    },
    {
      "source": "/boulder-fest-2025/artists",
      "destination": "/boulder-fest-2025-artists.html"
    },
    {
      "source": "/boulder-fest-2025/schedule",
      "destination": "/boulder-fest-2025-schedule.html"
    },
    {
      "source": "/boulder-fest-2025/gallery",
      "destination": "/boulder-fest-2025-gallery.html"
    },
    {
      "source": "/boulder-fest-2026",
      "destination": "/boulder-fest-2026-index.html"
    },
    {
      "source": "/boulder-fest-2026/artists",
      "destination": "/boulder-fest-2026-artists.html"
    },
    {
      "source": "/boulder-fest-2026/schedule",
      "destination": "/boulder-fest-2026-schedule.html"
    },
    {
      "source": "/boulder-fest-2026/gallery",
      "destination": "/boulder-fest-2026-gallery.html"
    },
    {
      "source": "/weekender-2026-09",
      "destination": "/weekender-2026-09-index.html"
    },
    {
      "source": "/weekender-2026-09/artists",
      "destination": "/weekender-2026-09-artists.html"
    },
    {
      "source": "/weekender-2026-09/schedule",
      "destination": "/weekender-2026-09-schedule.html"
    },
    {
      "source": "/weekender-2026-09/gallery",
      "destination": "/weekender-2026-09-gallery.html"
    },
    {
      "source": "/about-festival",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/donations",
      "destination": "/donations.html"
    },
    {
      "source": "/2026-artists",
      "destination": "/boulder-fest-2026-artists.html"
    },
    {
      "source": "/2026-schedule",
      "destination": "/boulder-fest-2026-schedule.html"
    },
    {
      "source": "/2026-gallery",
      "destination": "/boulder-fest-2026-gallery.html"
    },
    {
      "source": "/2025-artists",
      "destination": "/boulder-fest-2025-artists.html"
    },
    {
      "source": "/2025-schedule",
      "destination": "/boulder-fest-2025-schedule.html"
    },
    {
      "source": "/2025-gallery",
      "destination": "/boulder-fest-2025-gallery.html"
    },
    {
      "source": "/2026-sept-artists",
      "destination": "/weekender-2026-09-artists.html"
    },
    {
      "source": "/2026-sept-schedule",
      "destination": "/weekender-2026-09-schedule.html"
    },
    {
      "source": "/2026-sept-gallery",
      "destination": "/weekender-2026-09-gallery.html"
    },
    {
      "source": "/weekender-2026-09/:page",
      "destination": "/:page.html"
    },
    {
      "source": "/workshop-series-2026/:page",
      "destination": "/:page.html"
    },
    {
      "source": "/tickets",
      "destination": "/tickets.html"
    },
    {
      "source": "/success",
      "destination": "/success.html"
    },
    {
      "source": "/failure",
      "destination": "/failure.html"
    },
    {
      "source": "/my-tickets",
      "destination": "/my-tickets.html"
    },
    {
      "source": "/checkout-success",
      "destination": "/checkout-success.html"
    },
    {
      "source": "/checkout-cancel",
      "destination": "/checkout-cancel.html"
    },
    {
      "source": "/admin/checkin",
      "destination": "/admin/checkin.html"
    },
    {
      "source": "/manifest.json",
      "destination": "/public/manifest.json"
    }
  ],
  "redirects": [
    {
      "source": "/gallery-2025",
      "destination": "/2025-gallery",
      "permanent": false
    },
    {
      "source": "/boulder-fest-2025/tickets",
      "destination": "/tickets.html",
      "permanent": false
    },
    {
      "source": "/boulder-fest-2026/tickets",
      "destination": "/tickets.html",
      "permanent": false
    },
    {
      "source": "/weekender-2026-09/tickets",
      "destination": "/tickets.html",
      "permanent": false
    }
  ],
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Link",
          "value": "<https://fonts.googleapis.com>; rel=dns-prefetch"
        },
        { "key": "Link", "value": "<https://instagram.com>; rel=dns-prefetch" },
        {
          "key": "Link",
          "value": "<https://drive.google.com>; rel=preconnect; crossorigin"
        },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" },
        { "key": "Content-Security-Policy", "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://js.stripe.com https://www.googletagmanager.com https://www.google-analytics.com https://vercel.live; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://vercel.com; font-src 'self' data: https://fonts.gstatic.com https://vercel.live; img-src 'self' data: https: https://drive.google.com https://*.googleusercontent.com https://vercel.com; connect-src 'self' https://api.stripe.com https://m.stripe.com https://m.stripe.network https://api.brevo.com https://www.google-analytics.com https://vercel.live https://*.pusher.com wss://*.pusher.com; frame-src https://js.stripe.com https://hooks.stripe.com https://vercel.live; media-src 'self' https://drive.google.com https://*.googleusercontent.com; object-src 'none'; base-uri 'self'; form-action 'self' https://checkout.stripe.com; manifest-src 'self'" },
        { "key": "Permissions-Policy", "value": "camera=(), microphone=(), geolocation=(), interest-cohort=(), browsing-topics=()" }
      ]
    },
    {
      "source": "/(.*)\\.(jpg|jpeg|png|gif|svg|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        { "key": "Vary", "value": "Accept" }
      ]
    },
    {
      "source": "/(.*)\\.(css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, stale-while-revalidate=604800"
        },
        { "key": "Vary", "value": "Accept-Encoding" }
      ]
    },
    {
      "source": "/(.*)\\.(json)",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://alocubano.boulderfest.com"
        },
        { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    },
    {
      "source": "/js/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        { "key": "Service-Worker-Allowed", "value": "/" }
      ]
    }
  ]
}
