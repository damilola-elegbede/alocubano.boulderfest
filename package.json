{
  "name": "alocubano-boulderfest",
  "version": "1.0.0",
  "description": "A Lo Cubano Boulder Fest - Minimalist Dance Festival Website",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@latest",
  "scripts": {
    "//": "=== BUILD & DEVELOPMENT ===",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' node -e \"if(process.env.VERCEL === '1') { console.log('ðŸš€ Vercel build complete - no build needed for serverless functions'); } else { console.log('ðŸ”§ Local build - running verification'); require('child_process').execSync('npm run verify-structure', {stdio:'inherit'}); }\" && echo 'Build process completed'",
    "build:production": "NODE_OPTIONS='--max-old-space-size=4096' npm run build",
    "postbuild": "node -e \"if(process.env.VERCEL === '1') { console.log('ðŸš€ Vercel postbuild - skipping verify-structure'); } else { import('./build/verify-structure.js').catch(e => console.log('Verify-structure skipped:', e.message)); }\"",
    "verify-structure": "node build/verify-structure.js",
    "performance:audit": "lighthouse http://localhost:8000 --output-path=./performance-audit.html --save-assets",
    "//test": "=== TESTING ===",
    "test": "npm run test:simple",
    "test:simple": "vitest run --config tests/vitest.config.js",
    "test:simple:watch": "vitest --config tests/vitest.config.js",
    "test:coverage": "vitest run --coverage --config tests/vitest.config.js",
    "test:health": "node scripts/test-health-endpoint.js",
    "test:all": "NODE_OPTIONS='--max-old-space-size=3072' npm run test:simple && npm run test:e2e",
    "test:all:memory-optimized": "NODE_OPTIONS='--max-old-space-size=4096' npm run test:simple && npm run test:e2e",
    "test:e2e": "NODE_OPTIONS='--max-old-space-size=3072' playwright test --config=playwright-e2e-vercel-main.config.js",
    "test:e2e:ui": "NODE_OPTIONS='--max-old-space-size=3072' playwright test --ui --config=playwright-e2e-vercel-main.config.js",
    "test:e2e:headed": "NODE_OPTIONS='--max-old-space-size=3072' playwright test --headed --config=playwright-e2e-vercel-main.config.js",
    "test:e2e:debug": "NODE_OPTIONS='--max-old-space-size=3072' playwright test --debug --config=playwright-e2e-vercel-main.config.js",
    "test:e2e:fast": "NODE_OPTIONS='--max-old-space-size=3072' ALL_BROWSERS=false playwright test --config=playwright-e2e-vercel-main.config.js",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e:ci": "NODE_OPTIONS='--max-old-space-size=4096' DYNAMIC_PORT=${DYNAMIC_PORT:-3000} playwright test --config=playwright-e2e-vercel-main.config.js",
    "test:e2e:ci:standard": "NODE_OPTIONS='--max-old-space-size=3072' DYNAMIC_PORT=${DYNAMIC_PORT:-3000} E2E_TEST_SUITE=standard playwright test --config=playwright-e2e-vercel-main.config.js --grep='basic-navigation|cart-functionality|newsletter-simple'",
    "test:e2e:ci:advanced": "NODE_OPTIONS='--max-old-space-size=4096' DYNAMIC_PORT=${DYNAMIC_PORT:-3001} E2E_TEST_SUITE=advanced playwright test --config=playwright-e2e-vercel-main.config.js --grep='registration-flow|payment-flow|ticket-validation'",
    "test:e2e:ci:firefox": "NODE_OPTIONS='--max-old-space-size=3072' DYNAMIC_PORT=${DYNAMIC_PORT:-3002} E2E_TEST_SUITE=firefox playwright test --config=playwright-e2e-vercel-main.config.js --project=firefox",
    "test:e2e:ci:performance": "NODE_OPTIONS='--max-old-space-size=6144' DYNAMIC_PORT=${DYNAMIC_PORT:-3003} E2E_TEST_SUITE=performance PERFORMANCE_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep='gallery-browsing|user-engagement'",
    "test:e2e:ci:accessibility": "NODE_OPTIONS='--max-old-space-size=3072' DYNAMIC_PORT=${DYNAMIC_PORT:-3004} E2E_TEST_SUITE=accessibility ACCESSIBILITY_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep='mobile-registration-experience'",
    "test:e2e:ci:security": "NODE_OPTIONS='--max-old-space-size=3072' DYNAMIC_PORT=${DYNAMIC_PORT:-3005} E2E_TEST_SUITE=security SECURITY_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep='admin-auth|admin-dashboard'",
    "test:e2e:advanced:scenarios": "NODE_OPTIONS='--max-old-space-size=4096' ADVANCED_SCENARIOS=true playwright test --config=playwright-e2e-vercel-main.config.js",
    "test:e2e:accessibility:scenarios": "NODE_OPTIONS='--max-old-space-size=3072' ACCESSIBILITY_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep=\"accessibility\"",
    "test:e2e:performance:scenarios": "NODE_OPTIONS='--max-old-space-size=6144' PERFORMANCE_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep=\"performance|load\"",
    "test:e2e:security:scenarios": "NODE_OPTIONS='--max-old-space-size=3072' SECURITY_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep=\"security|admin|stripe\"",
    "test:e2e:wallet": "NODE_OPTIONS='--max-old-space-size=3072' playwright test --config=playwright-e2e-vercel-main.config.js --grep=\"wallet-pass\"",
    "test:e2e:database": "NODE_OPTIONS='--max-old-space-size=3072' playwright test --config=playwright-e2e-vercel-main.config.js --grep=\"database-integrity\"",
    "test:e2e:network": "NODE_OPTIONS='--max-old-space-size=3072' playwright test --config=playwright-e2e-vercel-main.config.js --grep=\"network-resilience\"",
    "test:e2e:email": "NODE_OPTIONS='--max-old-space-size=3072' playwright test --config=playwright-e2e-vercel-main.config.js --grep=\"email-transactional\"",
    "test:e2e:ngrok": "NODE_OPTIONS='--max-old-space-size=3072' node scripts/e2e-with-ngrok.js",
    "test:e2e:ngrok:ui": "NODE_OPTIONS='--max-old-space-size=3072' node scripts/e2e-with-ngrok.js -- --ui",
    "test:e2e:ngrok:headed": "NODE_OPTIONS='--max-old-space-size=3072' node scripts/e2e-with-ngrok.js -- --headed",
    "test:e2e:ngrok:debug": "NODE_OPTIONS='--max-old-space-size=3072' node scripts/e2e-with-ngrok.js -- --debug",
    "test:e2e:ngrok:fast": "NODE_OPTIONS='--max-old-space-size=3072' ALL_BROWSERS=false node scripts/e2e-with-ngrok.js",
    "test:e2e:validate": "node scripts/validate-e2e-setup.js",
    "test:e2e:setup": "node scripts/setup-e2e-testing.js",
    "test:e2e:setup:interactive": "node scripts/setup-e2e-testing.js interactive",
    "vercel:validate": "node scripts/validate-vercel-config.js",
    "test:e2e:standard": "NODE_OPTIONS='--max-old-space-size=3072' E2E_TEST_SUITE=standard PORT=3000 playwright test --config=playwright-e2e-vercel-main.config.js --grep='basic-navigation|cart-functionality|newsletter-simple'",
    "test:e2e:advanced": "NODE_OPTIONS='--max-old-space-size=4096' E2E_TEST_SUITE=advanced PORT=3001 playwright test --config=playwright-e2e-vercel-main.config.js --grep='registration-flow|payment-flow|ticket-validation'",
    "test:e2e:firefox": "NODE_OPTIONS='--max-old-space-size=3072' E2E_TEST_SUITE=firefox PORT=3002 playwright test --config=playwright-e2e-vercel-main.config.js --project=firefox",
    "test:e2e:performance": "NODE_OPTIONS='--max-old-space-size=6144' E2E_TEST_SUITE=performance PORT=3003 PERFORMANCE_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep='gallery-browsing|user-engagement'",
    "test:e2e:accessibility": "NODE_OPTIONS='--max-old-space-size=3072' E2E_TEST_SUITE=accessibility PORT=3004 ACCESSIBILITY_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep='mobile-registration-experience'",
    "test:e2e:security": "NODE_OPTIONS='--max-old-space-size=3072' E2E_TEST_SUITE=security PORT=3005 SECURITY_TESTING=true playwright test --config=playwright-e2e-vercel-main.config.js --grep='admin-auth|admin-dashboard'",
    "lint": "npm run lint:js && npm run lint:html",
    "lint:js": "eslint --config config/eslint.config.js js/**/*.js",
    "lint:html": "htmlhint --config config/.htmlhintrc pages/**/*.html",
    "//preview": "=== PREVIEW DEPLOYMENT TESTING ===",
    "preview:extract-url": "node scripts/get-vercel-preview-url.js",
    "preview:extract-url:env": "node scripts/get-vercel-preview-url.js --write-env --env-file .env.preview",
    "preview:validate": "node scripts/validate-preview-environment.js",
    "preview:test": "node scripts/run-e2e-preview.js",
    "preview:test:headed": "node scripts/run-e2e-preview.js --headed",
    "preview:test:debug": "node scripts/run-e2e-preview.js --debug",
    "preview:test:chromium": "node scripts/run-e2e-preview.js --browser chromium",
    "preview:test:performance": "node scripts/run-e2e-preview.js --suite performance",
    "preview:test:security": "node scripts/run-e2e-preview.js --suite security",
    "preview:setup:validate": "node scripts/test-preview-setup.js",
    "//dev": "=== DEVELOPMENT SERVERS ===",
    "dev:e2e": "node scripts/vercel-dev-e2e.js",
    "prestart": "npm run migrate:up",
    "start": "node scripts/start-with-ngrok.js",
    "prestart:local": "npm run migrate:up",
    "start:local": "npx http-server . -p 8080",
    "start:clean": "rm -rf .vercel && node scripts/vercel-dev-wrapper.js",
    "start:ci": "node scripts/vercel-dev-ci.js",
    "start:ci:port": "DYNAMIC_PORT=${DYNAMIC_PORT:-3000} node scripts/vercel-dev-ci.js",
    "vercel:dev": "vercel dev --yes --listen 3000",
    "vercel:dev:ci": "node scripts/vercel-dev-ci.js",
    "serve:simple": "npx http-server . -p 8000",
    "//database": "=== DATABASE & MIGRATIONS ===",
    "migrate:up": "DOTENV_CONFIG_PATH=.env.local node -r dotenv/config scripts/migrate.js",
    "migrate:status": "node scripts/migrate.js status",
    "migrate:verify": "node scripts/migrate.js verify",
    "migrate:e2e:up": "node scripts/migrate-e2e.js up",
    "migrate:e2e:status": "node scripts/migrate-e2e.js status",
    "db:shell": "sqlite3 data/development.db",
    "db:e2e:setup": "node scripts/setup-e2e-database.js setup",
    "db:e2e:clean": "node scripts/setup-e2e-database.js clean",
    "db:e2e:reset": "node scripts/setup-e2e-database.js reset",
    "health:check": "node scripts/test-health-endpoint.js",
    "health:database": "node scripts/test-database-health.js",
    "//optimization": "=== CI OPTIMIZATION & DEPENDENCIES ===",
    "quality:gates": "node scripts/quality-gates.js local",
    "quality:gates:ci": "node scripts/quality-gates.js ci",
    "quality:gates:report": "node scripts/quality-gates.js report",
    "quality:gates:dashboard": "node scripts/quality-gates.js dashboard",
    "ci:optimize": "node scripts/ci-performance-optimizer.js",
    "npm:optimize": "node scripts/optimize-npm-ci.js",
    "npm:optimize:config": "node scripts/optimize-npm-ci.js config",
    "npm:optimize:analyze": "node scripts/optimize-npm-ci.js analyze",
    "manage:deps": "node scripts/manage-dependencies.js",
    "manage:deps:analyze": "node scripts/manage-dependencies.js analyze",
    "manage:deps:security": "node scripts/manage-dependencies.js security",
    "manage:deps:update": "node scripts/manage-dependencies.js update",
    "manage:deps:optimize": "node scripts/manage-dependencies.js optimize",
    "manage:deps:report": "node scripts/manage-dependencies.js report",
    "validate:ci": "node scripts/validate-ci-quick.js",
    "validate:ci:full": "node scripts/validate-ci-optimization.js",
    "validate:ci:verbose": "node scripts/validate-ci-optimization.js --verbose",
    "validate:ci:export": "node scripts/validate-ci-optimization.js --export-json",
    "validate:port-config": "node scripts/validate-port-standardization.js",
    "//deploy": "=== DEPLOYMENT ===",
    "deploy:check": "NODE_OPTIONS='--max-old-space-size=3072' node scripts/deployment-check.js",
    "deploy:staging": "NODE_OPTIONS='--max-old-space-size=3072' npm run lint && npm run test:all && vercel --target preview",
    "deploy:production": "NODE_OPTIONS='--max-old-space-size=4096' npm run lint && npm run test:all && vercel --target production",
    "pre-commit": "npm run lint && npm test"
  },
  "dependencies": {
    "@libsql/client": "^0.15.10",
    "@sentry/node": "^7.84.0",
    "@stripe/stripe-js": "^7.8.0",
    "@types/node": "^24.2.0",
    "bcryptjs": "^3.0.2",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^10.0.0",
    "google-auth-library": "^10.2.1",
    "googleapis": "^153.0.0",
    "helmet": "^8.0.0",
    "html5-qrcode": "^2.3.8",
    "ioredis": "^5.7.0",
    "ip-range-check": "^0.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "passkit-generator": "^3.4.0",
    "qrcode": "^1.5.4",
    "redis": "^4.7.1",
    "sharp": "^0.34.3",
    "speakeasy": "^2.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "stripe": "^18.4.0",
    "thirty-two": "^1.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@playwright/test": "^1.49.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.18.0",
    "express": "^5.1.0",
    "globals": "^15.14.0",
    "htmlhint": "^1.6.3",
    "http-server": "^14.1.1",
    "lighthouse": "^12.3.0",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.1.10",
    "vercel": "^39.3.0",
    "vitest": "^2.1.9"
  },
  "description-features": {
    "architecture": [
      "Serverless API with Vercel functions",
      "Vanilla JavaScript frontend (ES6 modules)",
      "SQLite for development, Turso for production",
      "Redis for caching (ioredis)",
      "Google Drive gallery integration",
      "Stripe payment processing",
      "Apple & Google Wallet passes"
    ],
    "testing": [
      "Simple unit tests with Vitest - 26 essential tests",
      "Advanced E2E tests with Playwright - 26 comprehensive flows",
      "SQLite for unit tests, Turso for E2E tests",
      "Advanced scenarios: accessibility, performance, security, wallet integration",
      "Smart test suite selection with CI optimization",
      "Direct API testing with minimal abstractions"
    ]
  }
}